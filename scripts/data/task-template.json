{
    "family": "raft-example-server-$ID",
    "networkMode": "host",
    "containerDefinitions": [
        {
            "essential": true,
            "image": "tnachen\/raft-example-server",
            "cpu": 2048,
            "memory": 2048,
            "name": "raft-example-server",
            "command": ["./raft-example-server", "$ID", "$HTTP_PORT", "$RAFT_ADDRESS", "$LEADER_ADDRESS"],
            "ulimits": [
                {
                    "softLimit": 12000,
                    "hardLimit": 12000,
                    "name": "nofile"
                }
            ]
        }
    ]
}
